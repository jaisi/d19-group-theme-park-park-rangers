<div class="modal-dialog" role="document">
    <div class="modal-content" style="background-color:orange;">
      <div class="modal-header">
        <h5 class="modal-title">

         <div class='modal-body1'>
    				{{#each this}}
    				<section id="sec--{{id}}" class="help" style="background-color:{{colorTheme}}">
    					<div class="col-md-6 wrapper">
    						<div class="card-text-center">
    							<div class='card-block'>
                                  <h2>{{type}}</h2>
                                   {{#each attractions}}
    								<h4 id="card--{{id}} "class="card-title"><a>{{name}}</a></h4>
    								<h6 class="time-size-hbs">{{times}}</h6>
    								<h4 id="desc--{{id}}" class="hide-description">{{description}}</h4>
    								{{/each}}
    							</div>
    						</div>
    					</div>
    				</section>
    				{{/each}}
        </div>
        </h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body">
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
      </div>
    </div>
  </div>
